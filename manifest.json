{ 
  "name": "Chrome Automation", 
  "version": "1.0", 
  "description": "Automates browser functionality", 
  "manifest_version": 2,
  "browser_action" : {
	  "default_popup": "html/popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [ 
    "<all_urls>",
	  "tabs"
  ] 
}